    "^@/ui/(.*)$",
    "^@/hooks/(.*)$",
    "^@/shared/(.*)$",
    "^@/configs/(.*)$",
    "^@/services/(.*)$",
    "^@/utils/(.*)$",
    "^@/store/(.*)$",
    "^@/assets/(.*)$",
    "^@/providers/(.*)$",
    "^../(.*)",
    "^./(.*)",
    "(.scss)$"
